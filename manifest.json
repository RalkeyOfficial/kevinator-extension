{
    "manifest_version": 2,
    "name": "Kevinator",
    "description": "change all the images in to kevin",
    "icons": {
        "16": "icons/icon-16.png",
        "48": "icons/icon-48.png",
        "128": "icons/icon-128.png"
    },
    "version": "1.0",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["src/jquery.js", "src/content.js"]
        }
    ]
}